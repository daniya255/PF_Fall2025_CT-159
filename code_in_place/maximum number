#include<stdio.h>
int main()
{
    int num1,num2;
    
    printf("Enter your first number : ");
    scanf("%d",&num1);
    
    printf("Enter your second number : ");
    scanf("%d",&num2);
    
    if(num1==num2)
    {
                  printf("Your entered numbers are equal.");
     }
     else if(num1>num2)
     {
          printf("The first number is greater.");
     }
     else
     {
         printf("The second number is greater.");
     }
    
     
     getchar();
     getchar();
     return 0;
     
}   
